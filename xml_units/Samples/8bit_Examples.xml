<?xml version="1.0" encoding="UTF-8"?>
            
<UnitDescription>

<General>
  <UnitName>8-bit Images</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>run("PARA_STRING_1");</ImageJSyntax> 
  <Color>0x9cba92</Color>
  <HelpString>Opens one of ImageJ's 8-bit Image Examples</HelpString>
  	  
</General>

<Parameters>
  	<Parameter>
  		<Name>Sample</Name>
  		<DataType>StringArray</DataType>
  		<Value>Boats (356K);Bridge (174K);Dot Blot (7K);Fly Brain (1MB);Gel (105K);Particles (75K);TEM Filter (112K)</Value>
  		<HelpString>Selects the Images that will be loaded.</HelpString>
  		<ChoiceNumber>0</ChoiceNumber>
	</Parameter>
</Parameters>

<Outputs> 
	<Output>
		<Name>Output</Name>
		<ShortName>O</ShortName>	
		<DataType>Image</DataType>
		<ImageType>16</ImageType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>