<?xml version="1.0" encoding="UTF-8"?>
            
<UnitDescription>

<General>
  <UnitName>Round</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>if ("PARA_STRING_1" == "round") OUTPUT_INTEGER_1 = round(INPUT_NUMBER_1); else if ("PARA_STRING_1" == "ceil") OUTPUT_INTEGER_1 = floor(INPUT_NUMBER_1+1); else if ("PARA_STRING_1" == "floor") OUTPUT_INTEGER_1 = floor(INPUT_NUMBER_1);</ImageJSyntax> 
  <Color>0x9cba92</Color>
  <HelpString>Rounds a Double to an Integer</HelpString>
</General>

<Parameters>
	<Parameter>
		<Name>Mode</Name>
		<DataType>StringArray</DataType>
		<Value>ceil;round;floor</Value>
		<HelpString>Exact Rounding Mode (ceil, floor, floor)</HelpString>
		<ChoiceNumber>1</ChoiceNumber></Parameter></Parameters><Inputs>
<Input>
		<Name>Number</Name>
		<ShortName>N</ShortName>	
		<DataType>Number</DataType>
		<Required>true</Required>
		<NeedToCopyInput>false</NeedToCopyInput>
</Input>
</Inputs>
	<Outputs>
		
		<Output>
			<Name>Output</Name>
			<ShortName>O</ShortName><DataType>Integer</DataType>
			<DoDisplay>false</DoDisplay></Output></Outputs>

</UnitDescription>