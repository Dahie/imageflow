<?xml version="1.0" encoding="UTF-8"?>
<!--This is the unit to add noise to an image. (No parameters to be set.)-->
            
<UnitDescription>

<General>
  <UnitName>Find Edges</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>run("Find Edges");</ImageJSyntax> 
  <Color>0x6e91de</Color>
  <HelpString>Pronounces the edges in the image.</HelpString>
</General>

<Inputs> 
	<Input>
		<Name>Input</Name>
		<ShortName>I</ShortName>	
		<ImageType>31</ImageType>
		<NeedToCopyInput>true</NeedToCopyInput>
	</Input>
</Inputs>

<Outputs> 
	<Output>
		<Name>Output</Name>
		<ShortName>O</ShortName>	
		<ImageType>-1</ImageType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>