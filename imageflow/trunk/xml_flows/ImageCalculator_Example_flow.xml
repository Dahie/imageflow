<?xml version="1.0" encoding="UTF-8"?>
<FlowDescription>
  <Units>
    <Unit>
      <XPos>450</XPos>
      <YPos>100</YPos>
      <Label>Add Noise</Label>
      <UnitID>44</UnitID>
      <UnitDescription>
        <General>
          <UnitName>Add Noise</UnitName>
          <PathToIcon>AddNoise_Unit.png</PathToIcon>
          <ImageJSyntax>run("Add Noise" PARA_BOOLEAN_1);</ImageJSyntax>
          <Color>0x6e91de</Color>
          <IconSize>BIG</IconSize>
          <HelpString>Adds noise to an image</HelpString>
        </General>
        <Parameters>
          <Parameter>
            <Name>Stack</Name>
            <DataType>boolean</DataType>
            <HelpString>Stack support</HelpString>
            <Value>false</Value>
            <TrueString>, "stack"</TrueString>
          </Parameter>
        </Parameters>
        <Inputs>
          <Input>
            <Name>Input Image</Name>
            <ShortName>I</ShortName>
            <Required>true</Required>
            <DataType>Image</DataType>
            <ImageType>31</ImageType>
            <NeedToCopyInput>true</NeedToCopyInput>
          </Input>
        </Inputs>
        <Outputs>
          <Output>
            <Name>Output Image</Name>
            <ShortName>O</ShortName>
            <DataType>Image</DataType>
            <ImageType>16</ImageType>
            <DoDisplay>true</DoDisplay>
          </Output>
        </Outputs>
      </UnitDescription>
    </Unit>
    <Unit>
      <XPos>180</XPos>
      <YPos>50</YPos>
      <Label>Gaussian Blur</Label>
      <UnitID>42</UnitID>
      <UnitDescription>
        <General>
          <UnitName>Gaussian Blur</UnitName>
          <PathToIcon>GaussianBlur_Unit.png</PathToIcon>
          <ImageJSyntax>run("Gaussian Blur...", "sigma=PARA_DOUBLE_1");</ImageJSyntax>
          <Color>0x6e91de</Color>
          <IconSize>BIG</IconSize>
          <HelpString>Performs a gaussian blur</HelpString>
        </General>
        <Parameters>
          <Parameter>
            <Name>Radius</Name>
            <DataType>double</DataType>
            <HelpString>Radius of the gaussian kernel</HelpString>
            <Value>4.0</Value>
          </Parameter>
        </Parameters>
        <Inputs>
          <Input>
            <Name>Input</Name>
            <ShortName>I</ShortName>
            <Required>true</Required>
            <DataType>Image</DataType>
            <ImageType>31</ImageType>
            <NeedToCopyInput>true</NeedToCopyInput>
          </Input>
        </Inputs>
        <Outputs>
          <Output>
            <Name>Output</Name>
            <ShortName>O</ShortName>
            <DataType>Image</DataType>
            <ImageType>16</ImageType>
            <DoDisplay>false</DoDisplay>
          </Output>
        </Outputs>
      </UnitDescription>
    </Unit>
    <Unit>
      <XPos>30</XPos>
      <YPos>100</YPos>
      <Label>81894109xt8.jpg</Label>
      <UnitID>41</UnitID>
      <UnitDescription>
        <General>
          <UnitName>Image Source</UnitName>
          <PathToIcon />
          <ImageJSyntax>open("PARA_STRING_1");</ImageJSyntax>
          <Color>0x9cba92</Color>
          <IconSize>BIG</IconSize>
          <HelpString>Opens a new image</HelpString>
        </General>
        <Parameters>
          <Parameter>
            <Name>Image</Name>
            <DataType>String</DataType>
            <HelpString>The source unit needs the path of an image file.</HelpString>
            <Value>/home/senff/Bilder/81894109xt8.jpg</Value>
          </Parameter>
        </Parameters>
        <Inputs />
        <Outputs>
          <Output>
            <Name>Output</Name>
            <ShortName>O</ShortName>
            <DataType>Image</DataType>
            <ImageType>16</ImageType>
            <DoDisplay>false</DoDisplay>
          </Output>
        </Outputs>
      </UnitDescription>
    </Unit>
    <Unit>
      <XPos>320</XPos>
      <YPos>100</YPos>
      <Label>Image Calculator</Label>
      <UnitID>43</UnitID>
      <UnitDescription>
        <General>
          <UnitName>Image Calculator</UnitName>
          <PathToIcon />
          <ImageJSyntax>run("Image Calculator...", "image1=TITLE_1 operation=PARA_STRING_1 image2=TITLE_2 create PARA_BOOLEAN_1");</ImageJSyntax>
          <Color>0x6e91de</Color>
          <IconSize>BIG</IconSize>
          <HelpString>Performs a mathematical calculation of two images</HelpString>
        </General>
        <Parameters>
          <Parameter>
            <Name>Math</Name>
            <DataType>StringArray</DataType>
            <HelpString>Defines what math should be used to merge both images</HelpString>
            <ChoiceNumber>1</ChoiceNumber>
            <Value>Add;Subtract;Multiply;Divide;AND;OR;XOR;</Value>
          </Parameter>
          <Parameter>
            <Name>32 bit result</Name>
            <DataType>boolean</DataType>
            <HelpString>create 32 bit result?</HelpString>
            <Value>true</Value>
            <TrueString>32-bit</TrueString>
          </Parameter>
        </Parameters>
        <Inputs>
          <Input>
            <Name>Input1</Name>
            <ShortName>I1</ShortName>
            <Required>true</Required>
            <DataType>Image</DataType>
            <ImageType>31</ImageType>
            <NeedToCopyInput>false</NeedToCopyInput>
          </Input>
          <Input>
            <Name>Input2</Name>
            <ShortName>I2</ShortName>
            <Required>true</Required>
            <DataType>Image</DataType>
            <ImageType>31</ImageType>
            <NeedToCopyInput>false</NeedToCopyInput>
          </Input>
        </Inputs>
        <Outputs>
          <Output>
            <Name>Output</Name>
            <ShortName>O</ShortName>
            <DataType>Image</DataType>
            <ImageType>16</ImageType>
            <DoDisplay>false</DoDisplay>
          </Output>
        </Outputs>
      </UnitDescription>
    </Unit>
    <Unit>
      <XPos>30</XPos>
      <YPos>40</YPos>
      <Label>my usual example</Label>
    </Unit>
  </Units>
