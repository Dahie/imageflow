      <FromUnitID>41</FromUnitID>
      <FromOutputNumber>1</FromOutputNumber>
      <ToUnitID>42</ToUnitID>
      <ToInputNumber>1</ToInputNumber>
    </Connection>
    <Connection>
      <FromUnitID>42</FromUnitID>
      <FromOutputNumber>1</FromOutputNumber>
