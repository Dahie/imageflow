<?xml version="1.0" encoding="UTF-8"?>
 
<FlowDescription> 
	<Units>
	<Unit>
		<UnitID>1</UnitID>
		<XPos>50</XPos>
		<YPos>50</YPos>
	
		<UnitDescription>
		
			<General>
	  			<UnitName>Image Source</UnitName> 
	  			<PathToIcon></PathToIcon> 
	  			<ImageJSyntax>open("PARA_STRING_1");</ImageJSyntax> 
	  			<Color>0x9cba92</Color>
	  			<HelpString>Opens a new image</HelpString>
			</General>
	
			<Parameters>
	  			<Parameter>
	  				<Name>Image</Name>
	  				<DataType>String</DataType>
	  				<Value>/Users/barthel/Applications/ImageJ/_images/zange1.png</Value>
	  				<HelpString>The source unit needs the path of an image file.</HelpString>
	  			</Parameter> 
			</Parameters>
	
			<Outputs> 
				<Output>
					<Name>Output</Name>
					<ShortName>O</ShortName>	
					<ImageType>-1</ImageType>
					<DoDisplay>false</DoDisplay>
				</Output>
			</Outputs>
	
		</UnitDescription>
	</Unit>
	
	<Unit>
		<UnitID>2</UnitID> 
		<XPos>200</XPos>
		<YPos>50</YPos>
            
		<UnitDescription>
	
			<General>
	  			<UnitName>Gaussian Blur</UnitName> 
	  			<PathToIcon></PathToIcon> 
	  			<ImageJSyntax>run("Gaussian Blur...", "sigma=PARA_DOUBLE_1");</ImageJSyntax> 
	  			<Color>0x6e91de</Color>
	  			<HelpString>Performs a gaussian blur</HelpString>
	  		</General>
	
			<Parameters>
	  			<Parameter>
	  				<Name>Radius</Name>
	  				<DataType>double</DataType>
	  				<Value>4</Value>
	  				<HelpString>Radius of the gaussian kernel</HelpString>
	  			</Parameter> 
			</Parameters>
	
			<Inputs> 
				<Input>
					<Name>Input</Name>
					<ShortName>I</ShortName>	
					<ImageType>-1</ImageType>
					<NeedToCopyInput>true</NeedToCopyInput>
				</Input>
			</Inputs>
	
			<Outputs> 
				<Output>
					<Name>Output</Name>
					<ShortName>O</ShortName>	
					<ImageType>-1</ImageType>	
					<DoDisplay>true</DoDisplay>
				</Output>
			</Outputs>

		</UnitDescription>
	</Unit>
	
	<Unit>
		<UnitID>3</UnitID> 
		<XPos>350</XPos>
		<YPos>50</YPos>
            
		<UnitDescription>

			<General>
  				<UnitName>to 8 Bit</UnitName> 
  				<HelpString>Converts the image to an 8 bit image</HelpString>  
  				<PathToIcon></PathToIcon> 
  				<ImageJSyntax>run("8-bit");</ImageJSyntax> 
  				<Color>0x6e91de</Color>
			</General>

			<Inputs> 
				<Input>
					<Name>Input</Name>
					<ShortName>I</ShortName>	
					<ImageType>-1</ImageType>
					<NeedToCopyInput>true</NeedToCopyInput>
				</Input>	
			</Inputs>
	
			<Outputs> 
				<Output>
					<Name>Output</Name>
					<ShortName>O</ShortName>	
					<ImageType>-1</ImageType>
					<DoDisplay>false</DoDisplay>
				</Output>
			</Outputs>
	
		</UnitDescription>
	</Unit>

	<Unit>
		<UnitID>4</UnitID> 
		<XPos>500</XPos>
		<YPos>50</YPos>
            
		<UnitDescription>

			<General>
  				<UnitName>Fire</UnitName> 
 				<HelpString>Applies the fire LUT</HelpString>  
  				<PathToIcon></PathToIcon> 
  				<ImageJSyntax>run("Fire");</ImageJSyntax> 
  				<Color>0xbe91de</Color>
			</General>

			<Inputs> 
				<Input>
					<Name>Input1</Name>
					<ShortName>I</ShortName>	
					<ImageType>-1</ImageType>
					<NeedToCopyInput>true</NeedToCopyInput>
				</Input>	
			</Inputs>

			<Outputs> 
				<Output>
					<Name>Output</Name>
					<ShortName>O</ShortName>	
					<ImageType>-1</ImageType>
					<DoDisplay>true</DoDisplay>
				</Output>
			</Outputs>

		</UnitDescription>
	</Unit>
	
	</Units>
	
	<Connections>
		<Connection>
			<FromUnitID>1</FromUnitID> 
			<FromOutputNumber>1</FromOutputNumber>
			<ToUnitID>2</ToUnitID> 
			<ToInputNumber>1</ToInputNumber>
		</Connection>
		
		<Connection>
			<FromUnitID>2</FromUnitID> 
			<FromOutputNumber>1</FromOutputNumber>
			<ToUnitID>3</ToUnitID> 
			<ToInputNumber>1</ToInputNumber>
		</Connection>
		
		<Connection>
			<FromUnitID>3</FromUnitID> 
			<FromOutputNumber>1</FromOutputNumber>
			<ToUnitID>4</ToUnitID> 
			<ToInputNumber>1</ToInputNumber>
		</Connection>
	</Connections>

</FlowDescription>