<?xml version="1.0" encoding="UTF-8"?>
<!-- Shadows -->
            
<UnitDescription>

<General>
  <UnitName>Shadows</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>run("PARA_STRING_1", "STACK");</ImageJSyntax> 
  <Color>0x6e91de</Color>
  <HelpString>Renders shadows into the image.</HelpString>
  <IconSize>BIG</IconSize>  
</General>

<Parameters>
  <Parameter>
  	<Name>Math</Name>
  	<DataType>StringArray</DataType>
  	<Value>North;Northeast;East;Southeast;South;Southwest;West;Northwest</Value>
  	<HelpString>Defines what math operation will be performed</HelpString>
  	<ChoiceNumber>0</ChoiceNumber>
  </Parameter>
</Parameters>

<Inputs> 
  <Input>
	<Name>Image</Name>
	<ShortName>I</ShortName>
	<DataType>Image</DataType>	
	<ImageType>63</ImageType>
	<Required>true</Required>
	<NeedToCopyInput>true</NeedToCopyInput>
  </Input>
</Inputs>

<Outputs> 
  <Output>
	<Name>Output</Name>
	<ShortName>O</ShortName>
	<DataType>Image</DataType>	
	<ImageType>-1</ImageType>
	<DoDisplay>false</DoDisplay>
  </Output>
</Outputs>

</UnitDescription>