<?xml version="1.0" encoding="UTF-8"?>
<!-- UnitFile.xml -->
            
<UnitDescription>

<General>
  <UnitName>Background</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>newImage("Background", "8-bit White", PARA_INTEGER_1, PARA_INTEGER_2, 1);</ImageJSyntax> 
  <Color>0x9cba92</Color>
  <HelpString>Creates a new Background of defined size</HelpString>
  	  
</General>

<Parameters>
  	<Parameter>
  		<Name>Width</Name>
  		<DataType>integer</DataType>
  		<Value>256</Value>
  		<HelpString>Width of the background</HelpString>
  	</Parameter> 
  	<Parameter>
  		<Name>Height</Name>
  		<DataType>integer</DataType>
  		<Value>256</Value>
  		<HelpString>Height of the background</HelpString>
  	</Parameter> 
  	<Parameter>
  		<Name>Bitdepth</Name>
  		<DataType>StringArray</DataType>
  		<Value>32-bit;16-bit;8-bit RGB</Value>
  		<HelpString>Defines the bitdepth of the new image background.</HelpString>
  		<ChoiceNumber>1</ChoiceNumber></Parameter>
</Parameters>

<Outputs> 
	<Output>
		<Name>Output</Name>
		<ShortName>O</ShortName>
		<ImageType>-1</ImageType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>