<?xml version="1.0" encoding="UTF-8"?>
            
<UnitDescription>

<General>
  <UnitName>Max or Min</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>OUTPUT_NUMBER_1 = PARA_STRING_1Of(INPUT_NUMBER_1, INPUT_NUMBER_2);</ImageJSyntax> 
  <Color>0x9cba92</Color>
  <HelpString>Returns the bigger or the smaller of two values.</HelpString>
</General>

<Parameters>
  <Parameter>
  	 <Name>Math</Name>
  	 <DataType>StringArray</DataType>
  	 <Value>max; min</Value>
  	 <HelpString>Defines which mode should be used to compare both numbers.</HelpString>
     <ChoiceNumber>1</ChoiceNumber>
   </Parameter>
 </Parameters>

<Inputs>
<Input>
		<Name>First Number</Name>
		<ShortName>A</ShortName>	
		<DataType>Number</DataType>
		<NeedToCopyInput>false</NeedToCopyInput>
</Input>
	<Input>
		<Name>Second Number</Name>
		<ShortName>B</ShortName>
		<DataType>Number</DataType>
		<NeedToCopyInput>false</NeedToCopyInput></Input>
</Inputs>

<Outputs> 
<Output>
		<Name>Output</Name>
		<ShortName>C</ShortName>	
		<DataType>Number</DataType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>