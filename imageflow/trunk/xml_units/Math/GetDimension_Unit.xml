<?xml version="1.0" encoding="UTF-8"?>
            
<UnitDescription>

<General>
  <UnitName>Get Dimension</UnitName> 
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>OUTPUT_INTEGER_1 = getWidth();OUTPUT_INTEGER_2 = getHeight();</ImageJSyntax> 
  <Color>0x9cba92</Color>
  <HelpString>Get the Dimension of an input image.</HelpString>
</General>

<Parameters />

<Inputs>
<Input>
		<Name>Image</Name>
		<ShortName>I</ShortName>	
		<DataType>Image</DataType>
		<ImageType>31</ImageType>
		<NeedToCopyInput>false</NeedToCopyInput>
</Input>
</Inputs>

<Outputs> 
	<Output>
		<Name>Width</Name>
		<ShortName>W</ShortName>	
		<DataType>Integer</DataType>
		<DoDisplay>false</DoDisplay>
	</Output>
	<Output>
		<Name>Height</Name>
		<ShortName>H</ShortName>	
		<DataType>Integer</DataType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>