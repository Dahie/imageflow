<?xml version="1.0" encoding="UTF-8"?>
            
<UnitDescription>

<General>
  <UnitName>to 16 Bit</UnitName> 
  <HelpString>Converts the image to an 16 bit image</HelpString>  
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>run("16-bit");</ImageJSyntax> 
  <Color>0x6e91de</Color>
</General>

<Inputs> 
	<Input>
		<Name>Input</Name>
		<ShortName>I</ShortName>	
		<DataType>Image</DataType>
		<ImageType>31</ImageType>
		<NeedToCopyInput>true</NeedToCopyInput>
	</Input>	
</Inputs>

<Outputs> 
	<Output>
		<Name>Output</Name>
		<ShortName>O</ShortName>	
		<DataType>Image</DataType>
		<ImageType>4</ImageType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>