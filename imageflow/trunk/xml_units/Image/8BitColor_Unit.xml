<?xml version="1.0" encoding="UTF-8"?>
            
<UnitDescription>

<General>
  <UnitName>to 8 Bit Color</UnitName> 
  <HelpString>Converts the image to an 8 bit color image</HelpString>  
  <PathToIcon></PathToIcon> 
  <ImageJSyntax>run("8-bit Color", "number=PARA_INTEGER_1");</ImageJSyntax> 
  <Color>0x6e91de</Color>
</General>

<Parameters>
	<Parameter>
  		<Name>Number of colors</Name>
  		<DataType>integer</DataType>
  		<Value>256</Value>
  		<HelpString>How many colors should the image have? 2-256</HelpString>
  	</Parameter> 
</Parameters>

<Inputs> 
	<Input>
		<Name>Input</Name>
		<ShortName>I</ShortName>	
		<ImageType>31</ImageType>
		<NeedToCopyInput>true</NeedToCopyInput>
	</Input>	
</Inputs>

<Outputs> 
	<Output>
		<Name>Output</Name>
		<ShortName>O</ShortName>	
		<ImageType>2</ImageType>
		<DoDisplay>false</DoDisplay>
	</Output>
</Outputs>

</UnitDescription>